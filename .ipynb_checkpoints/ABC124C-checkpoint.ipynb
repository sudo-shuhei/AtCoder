    "S=input()\n",
    "N=len(S)\n",
    "list1=list(map(int,S))\n",
    "ans1=0\n",
    "for i in range(len(list1)):\n",
    "    if i%2==0:\n",
    "        if list1[i]==1:\n",
    "            ans1+=1\n",
    "    elif i%2==1:\n",
    "        if list1[i]==0:\n",
    "            ans1+=1\n",
    "ans2=min(ans1,(N-ans1))\n",
    "print(ans2)\n",
    "#AC"
   ]
  }
