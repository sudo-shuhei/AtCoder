   "source": [
    "N,K=map(int,input().split())\n",
    "if N>K:\n",
    "    ans=(N-K+1)/N\n",
    "else:\n",
    "    ans=0\n",
    "for j in range(N):\n",
    "    l=j\n",
    "    m=0\n",
    "    if j==0:\n",
    "        pass\n",
    "    while l<K:\n",
    "        l=2*j\n",
    "        m+=1\n",
    "    else:\n",
    "        ans+=(1/N)*1/(2**m)\n",
    "print(ans)"
   ]
  }
